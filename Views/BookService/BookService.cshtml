                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        <img src="~/Images/icon-notification.png" alt="">
                                    </a>
                                </li>
                                <li class="nav-item" style="color:white;"> | </li>
                                <li class="nav-item dropdown">
                                    <a class="nav-link " href="#" id="navbarDropdown" role="button" data-toggle="dropdown"
