                                <li class="nav__items border-btn"><a href="#myModal" data-toggle="modal" data-target="#myModal" data-dismiss="modal" class="nav__link nav-link custom-navitems custom-nav-btn">Login</a></li>
                                <li class="nav__items border-btn"><a asp-controller="home" asp-action="BecomeHelper" class="nav__link nav-link custom-nav-items custom-nav-btn">Become a Helper</a></li>
